datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

model User {
  id        String    @id @unique
  createdAt DateTime  @default(now())
  login     String    @unique
  name      String
  password  String
}